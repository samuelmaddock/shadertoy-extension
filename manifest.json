{
  "name": "Shadertoy Video Effects",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Apply shaders to videos on the web.",
  "browser_action": {
    "default_title": "Inject shader"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
